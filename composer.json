{
  "name": "macpaw/laravel-crowdin-integration",
  "description": "Automate translations uploading/downloading",
  "keywords": [
    "crowdin",
    "laravel"
  ],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^7.4",
    "laravel/framework": "^6.20.22",
    "elkuku/crowdin-api": "^2.0",
    "ext-zip": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "friendsofphp/php-cs-fixer": "2.14.*"
  },
  "autoload": {
    "psr-4": {
      "Maksi\\LaravelCrowdinIntegration\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maksi\\LaravelCrowdinIntegration\\Tests\\": "test/"
    }
  },
  "scripts": {
    "tests": [
      "vendor/bin/phpunit --verbose"
    ],
    "phpcs": [
      "vendor/bin/php-cs-fixer fix --config=.php_cs --verbose --diff --dry-run"
    ]
  },
  "minimum-stability": "dev"
}